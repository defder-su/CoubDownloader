LIST="channel_$1"
URL="https://coub.com/$1"
printf "[InternetShortcut]\nURL=$URL" > "$LIST.url"
wget "$URL" --output-document "$LIST.html"
./save_recoubs_by_html $LIST
if [ -e "$LIST.txt" ]; then
    echo "$LIST.txt already exist"
else
    ./coub.py -c $1 --write-list "$LIST.txt"
fi
if [ -e "$LIST.txt" ]; then
    ./save $LIST
else
    exit 1
fi
