CMDDIR="$(cd "$(dirname "$0")" && pwd)"
FN="recoubs_$1.txt"
cat $1.html | htmlq -t '#coubPageCoubJson' | jq --raw-output '.. | .coub_permalink? | strings' | sed -e "s/.*/https:\/\/coub.com\/view\/&/" > $FN
if [ -s "$FN" ]; then
    echo "Found recoubs for $1"
    $CMDDIR/save_with_recoubs recoubs_$1
else
    rm "$FN"
fi
